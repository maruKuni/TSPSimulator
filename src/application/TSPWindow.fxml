<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720" prefWidth="1280" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TSPController">
   <right>
      <VBox prefHeight="720.0" prefWidth="262.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <CheckBox fx:id="CheckBoxSA" mnemonicParsing="false" text="Simulated Anealing" />
                  <VBox prefHeight="307.0" prefWidth="280.0">
                     <children>
                        <Label text="初期温度" />
                        <Slider fx:id="SliderInitialTemper" blockIncrement="0.1" disable="true" majorTickUnit="0.1" max="1.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" />
                        <Label text="停止条件" />
                        <Slider fx:id="SliderStopTemper" blockIncrement="0.01" disable="true" majorTickUnit="0.1" max="1.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" />
                        <Label text="徐冷スケジュール" />
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Label text="回数" />
                              <TextField fx:id="textFieldSAIterate" disable="true" />
                              <Label text="乗数" />
                              <Slider fx:id="SliderSAmultipler" blockIncrement="0.01" disable="true" majorTickUnit="0.1" max="1.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" />
                           </children>
                           <padding>
                              <Insets left="5.0" />
                           </padding>
                        </VBox>
                        <ColorPicker prefHeight="42.0" prefWidth="125.0">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="10.0" />
                           </padding>
                        </ColorPicker>
                     </children>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </VBox>
               </children>
            </VBox>
         </children></VBox>
   </right>
   <center>
      <Canvas height="680.0" width="1000.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="5.0" />
         </BorderPane.margin>
      </Canvas>
   </center>
</BorderPane>
